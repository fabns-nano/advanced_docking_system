/* ADS close tab, close button & detach icons */

/* Undock  */
#undockButton {
  background: none;
  border: none;
	qproperty-icon: url(:/ads/images/ads_detach),
		url(:/ads/images/ads_detach_disabled) disabled;
	qproperty-iconSize: 16px;	
}

#undockButton:hover
{
  border: none;
  background:palette(Highlight);
}

/* Close  */
#closeButton {
  background: none;
  border: none;
	qproperty-icon: url(:/ads/images/ads_close),
		url(:/ads/images/ads_close_disabled) disabled;
	qproperty-iconSize: 16px;	
}

#closeButton:hover
{
  background:palette(Highlight);
}

#closeButton:pressed
{
  background:rgba(2, 174, 255, 50);
}

/* tabs menu button  */
#tabsMenuButton::menu-indicator
{
    image: none;
}

#tabsMenuButton {
  background: none;
  border: none;
  qproperty-icon: url(:/ads/images/ads_tab_menu),
		url(:/ads/images/ads_tab_menu_disabled) disabled;
	qproperty-iconSize: 16px;	
}

#tabsMenuButton:hover
{
  background:palette(Highlight);
}

#tabsMenuButton:pressed
{
  background:rgba(2, 174, 255, 50);
}