QFrame
{
    border: none;
}

/* Horizontal line */
.QFrame[frameShape="4"] {
  max-height: 1px;
  border: none;
  background-color: rgb(80, 80, 80);
}

/* Vertical line */
.QFrame[frameShape="5"] {
  max-width: 1px;
  border: none;
  background-color: rgb(80, 80, 80);
}


QFrame[border="true"]
{
    border: 1px solid rgb(80, 80, 80);
}

