QFrame
{
    border: none;
}

/* Horizontal line */
.QFrame[frameShape="4"] {
  max-height: 1px;
  border: none;
  background-color: rgb(81, 81, 81);
}

/* Vertical line */
.QFrame[frameShape="5"] {
  max-width: 1px;
  border: none;
  background-color: rgb(81, 81, 81);
}


QFrame[border="true"]
{
    border: 1px solid rgb(81, 81, 81);
}

/*===========================================================================
 * QMainWindow
 *===========================================================================*/
QMainWindow::separator 
{
    background: rgb(51, 51, 51);
    width: 6px;
}

QMainWindow::separator:hover 
{
    background: rgba(247, 138, 9, 50);
}
 
QMainWindow
{
    background: rgb(51, 51, 51);
}

/*===========================================================================
 * QSplitter
 *===========================================================================*/
QSplitterHandle:hover 
{
	
}


QSplitter::handle:horizontal
{
    image: url(:/visual/icons_darkstyle/splitter_horizontal);
    width: 9px;
}

QSplitter::handle:vertical
{
    image: url(:/visual/icons_darkstyle/splitter_vertical);
    height: 9px;
}


QSplitter::handle:hover
{
    /*background-color: rgb(71, 71, 71);*/
    background-color: rgba(247, 138, 9, 96);
}


/*===========================================================================
 * QPushButton
 *===========================================================================*/
 

QPushButton
{
    background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1, 
                                      stop:0 rgb(70, 70, 70), 
                                      stop:1 rgb(60, 60, 60));
    border: 1px solid rgb(81, 81, 81);
    /*min-height: 12px;*/
    padding: 5px;
}


/* QPushButton:focus
{
    border-color: palette(highlight);
} */

QPushButton:hover
{
    background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1, 
                                      stop:0 rgb(88, 88, 88), 
                                      stop:1 rgb(72, 72, 72));
}

QPushButton:hover:!focus
{
	border: 1px solid rgb(101, 101, 101);
}

QPushButton:pressed
{
    background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1, 
                                      stop:0 rgb(60, 60, 60), 
                                      stop:1 rgb(70, 70, 70));
    color: palette(highlight);
    border-color: rgb(81, 81, 81);
}


QPushButton:checked
{
    background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1, 
                                      stop:0 rgb(60, 60, 60), 
                                      stop:1 rgb(70, 70, 70));
    color: palette(highlight);
    border-color: palette(highlight);
}

QPushButton:checked:hover
{
    background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1, 
                                      stop:0 rgb(72, 72, 72), 
                                      stop:1 rgb(88, 88, 88));
}


/*===========================================================================
 * QToolButton
 *===========================================================================*/
QToolButton
{
	background-color: none;
    border: 1px solid transparent;
    padding: 0.2em;
}


QToolButton[vsmall="true"]
{
	padding: 0px;
	padding-left: 3px;
	padding-right: 3px;
}
 
 
QToolButton:hover
{
    border: 1px solid rgb(102, 102, 102);
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, 
                    stop:0 rgb(55, 55, 55), 
                    stop:1 rgb(44, 44, 44));
}

QToolButton:pressed
{
    background-color: rgb(81, 81, 81);
    color: palette(highlight);
    border-color: rgb(81, 81, 81);
}


QToolButton:checked:!pressed
{
    background-color: rgb(81, 81, 81);
    color: palette(highlight);
}

QToolButton[popupMode="1"] { /* only for MenuButtonPopup */
    padding-right: 17px; /* make way for the popup button */
}


QToolButton::menu-button {
    border-left: 1px solid transparent;
    /* 15px width + 1px for border = 16px allocated above */
    width: 15px;
}

QToolButton::menu-button:hover {
    border-left: 1px solid rgb(81, 81, 81);
}


QToolButton::menu-arrow {
    /* image: url(:/metro/images/toolbutton_down_arrow.png); */
    width: 9px;
    height: 9px;
}

QToolButton::menu-arrow:hover,
QToolButton::menu-arrow:hover:open
{
    /* image: url(:/metro/images/toolbutton_down_arrow_active.png); */
}


QToolButton::menu-arrow:open {
    top: 1px; left: 1px;
}

/*===========================================================================
* QtAdvancedDockingSystem
*===========================================================================*/

ads--CDockContainerWidget
{
	/* background: rgb(31, 31, 31); */
  background: palette(Dark);
} 

ads--CDockManager
{
	padding: 6px;
}

/* box que envolve todo widget */
ads--CDockAreaWidget
{
  border-top: none;
  background-color: palette(Window);
}

ads--CDockAreaTitleBar
{
	background: rgb(80, 80, 80);
	border-bottom: 1px solid palette(Highlight);
	min-height: 23px;
}

ads--CDockAreaTitleBar:disabled
{
	border-bottom: 1px solid rgb(140, 140, 140);
}

ads--CDockAreaTitleBar > QToolButton
{
	background: rgb(80, 80, 80);
	padding: 3 2px;
}

ads--CDockAreaTitleBar > QToolButton:hover
{
	background: rgb(91, 91, 91);
}

ads--CDockAreaTitleBar > QToolButton:pressed
{
	border: 1px solid palette(Highlight);
	background: rgba(255, 196, 125, 127);
}

ads--CDockAreaTabBar
{

}

