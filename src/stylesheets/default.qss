ads--CDockWidgetTab
{
    padding: 0 0px;
}

ads--CDockWidgetTab > QLabel
{
  color: #9a9a9a;
}

ads--CDockWidgetTab[activeTab="false"]
{
	background: rgb(80, 80, 80);
	border-right: 1px solid rgb(51, 51, 51);
}

ads--CDockWidgetTab:hover {
  background: palette(Highlight);
}

ads--CDockWidgetTab[activeTab="true"]
{
	background: palette(Highlight);
}

ads--CDockWidgetTab[activeTab="true"]:disabled
{
	background: rgb(140, 140, 140);
}

ads--CDockWidgetTab[activeTab="true"] #dockWidgetTabLabel
{
	color: rgb(255, 255, 255);
}

#tabCloseButton
{
	margin-top: 2px;
	background: none;
	border: none;
	padding: 2px 0px;
	qproperty-icon: url(:/ads/images/ads_close), url(:/ads/images/ads_close_disabled) disabled;
	qproperty-iconSize: 12px;
}

#tabCloseButton:hover
{
  border: 1px solid rgba(0, 0, 0, 32);
	background: rgba(0, 0, 0, 16);
}

#tabCloseButton:pressed
{
	background: rgba(0, 0, 0, 32);
}

ads--CDockWidget > QToolBar
{
  border-bottom: 1px solid palette(light);
}

ads--CDockWidget 
{
  border: 10px solid rgb(80, 80, 80);
  border-top: none;
  background: rgb(51, 51, 51);
}

/**
 * The border comes from the itemview that is inside of the dock widget so we
 * we can disable the border
 */
ads--CDockWidget[ItemViewContainer="true"]
{
	border-left: none;
	border-right: none;
	border-bottom: none;
}

ads--CDockWidget[ItemViewContainer="true"] QTableView
{
	gridline-color: rgb(80, 80, 80);
	padding-left: -1px;
	padding-right: -1px;
	background-color: rgb(46, 46, 46);
}

ads--CDockWidget
{
  /*
    background: palette(light);
    border-color: palette(light);
    border-style: solid;
    border-width: 1px 0 0 0;
  */
  margin: 0;
  padding:0;
  /* border-top: 1px solid #ff0000; */
  border:none;
  background-color: palette(Dark);
}

ads--CTitleBarButton
{
  padding: 0px 0px;
}

