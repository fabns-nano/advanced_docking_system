/*===========================================================================
 * QPushButton
 *===========================================================================*/
 

QPushButton
{
    background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1, 
                                      stop:0 rgb(70, 70, 70), 
                                      stop:1 rgb(60, 60, 60));
    border: 1px solid rgb(80, 80, 80);
    /*min-height: 12px;*/
    padding: 5px;
}


/* QPushButton:focus
{
    border-color: palette(highlight);
} */

QPushButton:hover
{
    background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1, 
                                      stop:0 rgb(88, 88, 88), 
                                      stop:1 rgb(72, 72, 72));
}

QPushButton:hover:!focus
{
	border: 1px solid rgb(101, 101, 101);
}

QPushButton:pressed
{
    background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1, 
                                      stop:0 rgb(60, 60, 60), 
                                      stop:1 rgb(70, 70, 70));
    color: palette(highlight);
    border-color: rgb(80, 80, 80);
}


QPushButton:checked
{
    background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1, 
                                      stop:0 rgb(60, 60, 60), 
                                      stop:1 rgb(70, 70, 70));
    color: palette(highlight);
    border-color: palette(highlight);
}

QPushButton:checked:hover
{
    background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1, 
                                      stop:0 rgb(72, 72, 72), 
                                      stop:1 rgb(88, 88, 88));
}


/*===========================================================================
 * QToolButton
 *===========================================================================*/
QToolButton
{
	background-color: none;
    border: 1px solid transparent;
    padding: 0.2em;
}


QToolButton[vsmall="true"]
{
	padding: 0px;
	padding-left: 3px;
	padding-right: 3px;
}
 
 
QToolButton:hover
{
    border: 1px solid rgb(102, 102, 102);
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, 
                    stop:0 rgb(55, 55, 55), 
                    stop:1 rgb(44, 44, 44));
}

QToolButton:pressed
{
    background-color: rgb(80, 80, 80);
    color: palette(highlight);
    border-color: rgb(80, 80, 80);
}


QToolButton:checked:!pressed
{
    background-color: rgb(80, 80, 80);
    color: palette(highlight);
}

QToolButton[popupMode="1"] { /* only for MenuButtonPopup */
    padding-right: 17px; /* make way for the popup button */
}


QToolButton::menu-button {
    border-left: 1px solid transparent;
    /* 15px width + 1px for border = 16px allocated above */
    width: 15px;
}

QToolButton::menu-button:hover {
    border-left: 1px solid rgb(80, 80, 80);
}


QToolButton::menu-arrow {
    /* image: url(:/metro/images/toolbutton_down_arrow.png); */
    width: 9px;
    height: 9px;
}

QToolButton::menu-arrow:hover,
QToolButton::menu-arrow:hover:open
{
    /* image: url(:/metro/images/toolbutton_down_arrow_active.png); */
}


QToolButton::menu-arrow:open {
    top: 1px; left: 1px;
}

/*===========================================================================
* QtAdvancedDockingSystem
*===========================================================================*/

ads--CDockContainerWidget
{
	background: rgb(31, 31, 31);
} 

ads--CDockManager
{
	padding: 6px;
}

/* box que envolve todo widget */
ads--CDockAreaWidget
{
  background: rgb(50, 50, 50);
  margin: 3px 3px 3px 3px;
  /* border:3px solid #ffff00;
  margin: 20px 20px 20px 20px; */
}

ads--CDockAreaTitleBar
{
	background: rgb(80, 80, 80);
	border-bottom: 2px solid palette(Highlight);
	min-height: 25px;
}

/* ads--CDockAreaTitleBar:disabled
{
	border-bottom: 1px solid rgb(140, 140, 140);
} */

ads--CDockAreaTitleBar > QToolButton
{
	background: rgb(80, 80, 80);
	padding: 3px 2px;
}

ads--CDockAreaTitleBar > QToolButton:hover
{
	background: rgb(91, 91, 91);
}

ads--CDockAreaTitleBar > QToolButton:pressed
{
	border: 1px solid palette(Highlight);
	background: rgba(255, 196, 125, 127);
}

ads--CDockAreaTabBar
{

}

ads--CDockWidgetTab
{
  background-color: rgb(80, 80, 80);
  padding: 2px 3px 2px 3px;
  border-top-left-radius:7px;
  border-top-right-radius:7px;
  margin:3px 0px 0px 3px;
}

ads--CDockWidgetTab > QLabel
{
  color: #9a9a9a;
}

ads--CDockWidgetTab[activeTab="false"]
{
	background: rgb(50, 50, 50);
	/* border-right: 1px solid rgb(50, 50, 50); */
}

ads--CDockWidgetTab:hover {
  background: rgb(230, 106, 58);
  color: rgb(255, 255, 255);
}

ads--CDockWidgetTab:hover > QLabel {
  color: rgb(255, 255, 255);
}

ads--CDockWidgetTab[activeTab="true"]
{
	background: palette(Highlight);
}

ads--CDockWidgetTab[activeTab="true"]:disabled
{
	background: rgb(140, 140, 140);
}

ads--CDockWidgetTab[activeTab="true"] #dockWidgetTabLabel
{
	color: rgb(255, 255, 255);
}

/* background of the ADS tabs */
QWidget#tabsContainerWidget {
  background: rgb(80, 80, 80);
}

#tabCloseButton
{
	margin-top: 2px;
	background: none;
	border: none;
	padding: 2px 0px;
	qproperty-icon: url(:/ads/images/ads_close), url(:/ads/images/ads_close_disabled) disabled;
	qproperty-iconSize: 12px;
}

#tabCloseButton:hover
{
  border: 1px solid rgba(0, 0, 0, 32);
	background: rgba(0, 0, 0, 16);
}

#tabCloseButton:pressed
{
	background: rgba(0, 0, 0, 32);
}

ads--CDockWidget > QToolBar
{
  border-bottom: 1px solid palette(light);
}

ads--CDockWidget 
{
  background: rgb(50, 50, 50);
  padding: 0px 0px 0px 0px;
  margin: 0px;
}

ads--CDockWidget[ItemViewContainer="true"] QTableView
{
	gridline-color: rgb(80, 80, 80);
	padding-left: -1px;
	padding-right: -1px;
	background-color: rgb(46, 46, 46);
}

ads--CTitleBarButton
{
  padding: 0px 0px;
}


